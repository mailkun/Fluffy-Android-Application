<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fluffy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fluffy.NoActionBar">
        <activity
            android:name=".activity_detail_burung"
            android:theme="@style/Theme.Fluffy"
            android:parentActivityName=".activity_dashboard"></activity>
      <activity android:name=".SignupActivity" />
      <activity android:name=".SignUpUiActivity">
          <intent-filter>
              <action android:name="android.intent.action.MAIN" />
              <category android:name="android.intent.category.LAUNCHER" />
          </intent-filter>
      </activity>
      <activity android:name=".MainActivity"></activity>
    </application>

</manifest>